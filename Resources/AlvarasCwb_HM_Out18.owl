<rdf:RDF xmlns="http://www.semanticweb.org/ricardo/ontologies/2016/5/alvarasCwb#"
     xml:base="http://www.semanticweb.org/ricardo/ontologies/2016/5/alvarasCwb"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
